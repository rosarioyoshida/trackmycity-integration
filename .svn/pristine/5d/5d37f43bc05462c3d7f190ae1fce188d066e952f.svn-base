package br.com.trackmycity.dao;

import java.util.List;

import javax.inject.Inject;
import javax.persistence.EntityManager;
import javax.persistence.criteria.CriteriaBuilder;
import javax.persistence.criteria.CriteriaQuery;
import javax.persistence.criteria.Order;
import javax.persistence.criteria.Root;

import br.com.trackmycity.models.AlertSubType;

public class AlertSubTypeDAO extends GenericDAO {
	
	private static final long serialVersionUID = 5043438520095163284L;
	private CommonOperationsDAO<AlertSubType, Long> commonDAO;

	@Inject
	public AlertSubTypeDAO(EntityManager em) {
		super(em);
		this.commonDAO = new CommonOperationsDAO<AlertSubType, Long>(em, AlertSubType.class);
	}

	public void persist(AlertSubType entity) {
		commonDAO.persist(entity);
	}

	public AlertSubType merge(AlertSubType entity) {
		return commonDAO.merge(entity);
	}

	public void remove(AlertSubType entity) {
		commonDAO.remove(entity);
	}

	public AlertSubType find(Long primaryKey) {
		return commonDAO.find(primaryKey);
	}

	public List<AlertSubType> list() {
		return commonDAO.list();
	}

	public List<AlertSubType> list(List<Order> orders) {
		return commonDAO.list(orders);
	}

	public CriteriaBuilder getCriteriaBuilder() {
		return commonDAO.getCriteriaBuilder();
	}

	public CriteriaQuery<AlertSubType> getCriteriaQuery() {
		return commonDAO.getCriteriaQuery();
	}

	public Root<AlertSubType> getRoot() {
		return commonDAO.getRoot();
	}
	
	

}
