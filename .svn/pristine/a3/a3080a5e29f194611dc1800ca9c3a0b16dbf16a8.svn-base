package br.com.trackmycity.dao;

import javax.inject.Inject;
import javax.persistence.EntityManager;

import br.com.trackmycity.models.AlertType;

public class AlertTypeDAO {
	
	private EntityManager em;
	private GenericDAO<AlertType, Long> genericDAO;

	@Inject
	public AlertTypeDAO(EntityManager em) {
		 this.genericDAO = new GenericDAO<AlertType, Long>(em, AlertType.class);
	}
	
	public void save(AlertType t) {
		genericDAO.save(t);
	}

	public AlertType findById(Long id) {
		return genericDAO.findById(id);
	}
	
	
}
